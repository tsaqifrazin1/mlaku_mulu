  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watchAll --config jest-unit-test.config.ts --runInBand",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm:win": "dotenv -e .env -- ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d src/utils/ormconfig.ts",
    "typeorm:generate:win": "npm run typeorm:win -- migration:generate",
    "typeorm:create:win": "npm run typeorm:win migration:create -- -n",
    "typeorm:run:win": "npm run typeorm:win migration:run",
    "typeorm:revert:win": "npm run typeorm:win migration:revert"
  },
